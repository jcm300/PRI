extends layout

block conteudo
    .w3-container
        .w3-card-4
            header.w3-container.w3-red
                h1 Tese Recebida
            .w3-container
                h3 Aluno
                table.w3-table-all
                    tr
                        th Nome
                        td= tese.nome
                    tr
                        th Número/Identificador
                        td= tese.numero
                    tr
                        th Curso
                        td= tese.curso
                    tr
                        th Instituição
                        td= tese.instituicao
                h3 Tese
                table.w3-table-all
                    tr
                        th Título
                        td= tese.titulo
                    tr
                        th Data
                        td= tese.data
                    tr
                        th Tipo
                        td= tese.tipo
            footer.w3-container.w3-red
                h6
                    a(href="http://localhost:4006/") Voltar à página principal
                address Gerado por RegTeseApp by José Carlos Lima Martins
